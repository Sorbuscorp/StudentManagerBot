[
  {
    "name": "help",
    "code": "/help",
    "description": "Print All commands, for detailed command info use '/help [command]' ",
    "access": [ "Lecturer", "Student", "Guest" ],
    "args": [ "CommandName" ], // this must be a command name
    "typeArgs": [ "string" ],
    "optional": [ "CommandName" ]
  },
  {
