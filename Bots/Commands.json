    "access": [ "Guest" ],
    "args": [
      "role", //values: lecturer | student
      "Family", //any type-satisfact
      "GroupName" //
    ],
    "typeArgs": [ "Permission", "string", "string" ],
    "optional": [ "GroupName" ]
  },
  {
    "name": "registredList",
    "code": "/registredList",
    "description": "Show list of registred users",
    "access": [ "Lecturer" ],
    "args": [ "WhoListed" ], // values: lecturer | student
    "typeArgs": [ "Permission" ],
    "optional": [ "WhoListed" ]
  },
  {
    "name": "myStudents",
    "code": "/myStudents",
    "description": "Show list of yours students",
    "access": [ "Lecturer" ],
    "args": [ "filter" ], //выбор по конкретной дисциплине или всех. values: discipline | gropup | all
    "typeArgs": [ "string" ],
    "optional": [ "filter" ]
  },
  {
    "name": "myDisciplines",
